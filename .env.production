# MongoDB Connection String (use your MongoDB Atlas connection string)
MONGODB_URI=mongodb+srv://YOUR_MONGODB_USER:YOUR_MONGODB_PASSWORD@YOUR_CLUSTER.mongodb.net/YOUR_DATABASE_NAME?appName=YOUR_APP_NAME

# Server Port (Vercel will set this automatically, but you can override)
PORT=3000

# API Base URL (your Vercel deployment domain)
VITE_API_URL=https://your-app.vercel.app
